{
  "name": "vue-forest-admin",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.html",
  "browser": {
    "vue": "vue/dist/vue.common"
  },
  "scripts": {
    "build:dev": "NODE_ENV=developement watchify -p browserify-hmr -t [ babelify --presets [ es2015 ] ] -t [ vueify ] -e src/app.js -o dist/index.js",
    "build:prod": "NODE_ENV='production' browserify -t [ babelify --presets [ es2015] ] -t [ vueify ] -t [ envify ] -e src/app.js -o dist/index.js",
    "index": "cp src/index.html dist/ ",
    "start": "yarn index && yarn style & yarn build:dev & cd dist && serve",
    "style": "node-sass src/main.style.scss dist/main.style.css -w"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "colors": "^1.1.2",
    "element-ui": "^1.2.4",
    "envify": "^4.0.0",
    "fastclick": "^1.0.6",
    "jquery": "^3.1.1",
    "vue": "^2.2.2",
    "vue-cookies": "^1.5.4",
    "vue-resource": "^1.3.4",
    "vue-router": "^2.3.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-vue": "^0.1.0",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "browserify-hmr": "^0.3.5",
    "node-sass": "^4.5.3",
    "vueify": "^9.4.1",
    "watchify": "^3.9.0"
  }
}
